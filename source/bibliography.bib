@article{tranSupportMathematicalThinking2017,
  title = {Support of Mathematical Thinking through Embodied Cognition: {{Nondigital}} and Digital Approaches},
  shorttitle = {Support of Mathematical Thinking through Embodied Cognition},
  author = {Tran, Cathy and Smith, Brandon and Buschkuehl, Martin},
