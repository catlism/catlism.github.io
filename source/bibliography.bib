  author = {Venturini, Tommaso and Rogers, Richard},
  year = {2019},
  month = apr,
  journal = {Digital Journalism},
  volume = {7},
  number = {4},
  pages = {532--540},
  issn = {2167-0811, 2167-082X},
  doi = {10.1080/21670811.2019.1591927},
  url = {https://www.tandfonline.com/doi/full/10.1080/21670811.2019.1591927},
  abstract = {API-based research is an approach to computational social sciences and digital sociology based on the extraction of records from the datasets made available by online platforms through their application programming interfaces (APIs). This type of research has allowed the collection of detailed information on large populations, thereby effectively challenging the distinction between qualitative and quantitative methods. Lately, access to this source of data has been significantly reduced after a series of scandals connected to the use of personal information. The closure of APIs, however, can have positive effects, if it encourages researchers to reduce their dependence on mainstream platforms and explore new sources and ways to collect online records that are closer to the digital fieldwork.},
  langid = {english},
  keywords = {catlism_copyright-ethics}
}

@article{vishwanathHabitualFacebookUse2015,
